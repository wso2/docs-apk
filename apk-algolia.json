{
  "index_name": "apk-algolia",
  "start_urls": [
    "https://docs.apk.io"
  ],
  "stop_urls": [],
  "sitemap_urls": [
    "https://docs.apk.io/sitemap.xml"
  ],
  "selectors": {
    "lvl0": {
      "selector": ".navbar > .site-nav > .nav-links.can-hide > .nav-item > a.nav-link.router-link-active",
      "global": true,
      "default_value": "Docs"
    },
    "lvl1": "p.sidebar-heading.open",
    "lvl2": ".theme-default-content h1",
    "lvl3": ".theme-default-content h2",
    "text": ".theme-default-content p, .theme-default-content li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "strip_chars": " .,;:#",
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  },
  "min_indexed_level": 2,
  "only_content_level": true
}
